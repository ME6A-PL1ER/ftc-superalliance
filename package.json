{
  "name": "ftc-superalliance",
  "version": "1.0.0",
  "description": "A scouting form website for FTC robotics using Node.js, React, and SQLite.",
  "main": "server/src/app.ts",
  "scripts": {
    "start": "node server/src/app.js",
    "client": "npm start --prefix client",
    "server": "nodemon server/src/app.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "concurrently": "^9.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "author": "Owen Rossing",
  "license": "MIT"
}